<h1>Listing product_batches</h1>

<table>
  <tr>
    <th>Worksheet</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_batches.each do |product_batch| %>
  <tr>
    <td><%= product_batch.worksheet %></td>
    <td><%= product_batch.code %></td>
    <td><%= link_to 'Show', product_batch %></td>
    <td><%= link_to 'Edit', edit_product_batch_path(product_batch) %></td>
    <td><%= link_to 'Destroy', product_batch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

