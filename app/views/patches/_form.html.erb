<%= simple_form_for(@patch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code %>
    <%= f.association :product_batch %>
    <%= f.input :ticket_count %>
    <%= f.input :pack_no_totals %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
